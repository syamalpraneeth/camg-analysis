#--------------------------------------------------------------VARIABLES FROM COMMAND LINE
variable        pot string ${args}
variable        pot string Cu-Zr_4.eam.fs
next            args
variable        e1 string ${args}
variable        e1 string Cu
next            args
variable        e2 string ${args}
variable        e2 string Zr
next            args
variable        x equal ${args}
variable        x equal 50
next            args
variable        y equal ${args}
variable        y equal 50
variable        frac equal $y/($x+$y)
variable        frac equal 50/($x+$y)
variable        frac equal 50/(50+$y)
variable        frac equal 50/(50+50)
next            args
variable        T1 equal ${args}
variable        T1 equal 200000
next            args
variable	heat_ramp equal ${args}			#0.05/ps ramp rate
variable	heat_ramp equal 0.25			

variable        th equal ${T1}/40			#thermo and dump values evaluated
variable        th equal 200000/40			
variable        df equal ${T1}/20
variable        df equal 200000/20

variable	lat equal 3.6				#lattice parameter to define spacings

variable	T_hot equal 2000
variable	T_cool equal 50
variable	T_heat equal 1000
#variable        T2 equal (${T_heat}-${T_cool})*((10^3)/(${quench_ramp})) #0.1K/ps quench rate at 1 atm pressure
variable	ht equal (${T_heat}-${T_cool})*((10^3)/(${heat_ramp})) #0.5K/ps quench rate at 1 atm pressure
variable	ht equal (1000-${T_cool})*((10^3)/(${heat_ramp})) 
variable	ht equal (1000-50)*((10^3)/(${heat_ramp})) 
variable	ht equal (1000-50)*((10^3)/(0.25)) 

variable        t1 equal ${T1}/10
variable        t1 equal 200000/10
variable        t equal ${ht}/25
variable        t equal 3800000/25
variable        ht_inter equal (${ht}/10)
variable        ht_inter equal (3800000/10)

#--------------------------------------------------------------------------------INTIALISE
units           metal					#fundamental LAMMPS and MD conditions initialization
dimension       3
atom_style      atomic
boundary        p p p
lattice		bcc ${lat}
lattice		bcc 3.6
Lattice spacing in x,y,z = 3.6000000 3.6000000 3.6000000

read_restart	../restart.msr_20
Reading restart file ...
