#-------------------------------------------------------------------------------HEATING
variable	loopsiz equal 20
variable	dT equal (${T_heat}-${T_cool})/${loopsiz}
variable	dT equal (1000-${T_cool})/${loopsiz}
variable	dT equal (1000-50)/${loopsiz}
variable	dT equal (1000-50)/20
variable	Temp1 equal ${T_cool}
variable	Temp1 equal 50
variable	Temp2 equal ${T_cool}+${dT}
variable	Temp2 equal 50+${dT}
variable	Temp2 equal 50+47.5
variable        tstop equal $(step)+${ht}
variable        tstop equal 0+${ht}
variable        tstop equal 0+19000

variable	hloop loop ${loopsiz}
variable	hloop loop 20
label		l3
	fix     heat all npt temp ${Temp1} ${Temp2} 0.1 iso 0 0 0.1
	fix     heat all npt temp 50 ${Temp2} 0.1 iso 0 0 0.1
	fix     heat all npt temp 50 97.5 0.1 iso 0 0 0.1
	run	${ht_inter}
